    content += "\n\n\n\n";
    content += "----Transmit Infomation----";
    content += "Sender: " + from + "\n";
    content += "Date: " + date + "\n";
    content += "To:";
    for(int i = 0; i < to.size(); ++i) {
        content += to[i];
    }
    content += "\n";
    content += "Subject: " + originalhead + "\n";
    content += originaltext;
}
